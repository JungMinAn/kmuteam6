n = int(input("Enter number: "))
def fibonacci(n):
    if(n<=1):
        return n
    else:
        return fibonacci(n-1) + fibonacci(n-2)

def fibonacci2(n):
    a, b = 1, 1
    i = 0
    while i<n:
        if (i+1==n):
            print(a)
            break
        else:
            a, b = b, a+b
            i+=1

print(fibonacci(n))
fibonacci2(n)